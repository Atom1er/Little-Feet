<div class="modal fade" id="add-child" tabindex="-1" role="dialog" aria-labelledby="exampleModalCenterTitle"
  aria-hidden="true">
  <div class="modal-dialog modal-dialog-centered" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title" id="exampleModalLongTitle">Add User
        </h5>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
      <div class="modal-body text-justify">

        <label for="child-name">Child Name</label>
        <input id="add-child" type="text" placeholder="Child's name here">
        <br>
        <label for="parent-name">Parent Name</label>
        <input id="add-parent" type="text" placeholder="Parent's name here">

      </div>
      <div class="modal-footer">
        <button id="get-one"> add child</button>
      </div>
    </div>
  </div>
</div>

<div class="modal fade" id="add-activity" tabindex="-1" role="dialog" aria-labelledby="exampleModalCenterTitle"
  aria-hidden="true">
  <div class="modal-dialog modal-dialog-centered" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title" id="exampleModalLongTitle">Add Activity
        </h5>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
      <div class="modal-body text-center">

        <div class="row">
          <div class="col-6">
            <label for="specific-child">Child Name</label>
            <input id="add-child-name2" type="text" placeholder="Specify child">
          </div>
          <div class="col-6">
            <label for="activity">Activity</label>
            <input id="add-activity" type="text" placeholder="Type activity">
          </div>
        </div>


      </div>
      <div class="modal-footer">
        <button id="get-activity" class="btn btn-dark"> add activity</button>
        <button id="find-activities" class="btn btn-dark">Find Child activities</button>
      </div>
    </div>
  </div>
</div>