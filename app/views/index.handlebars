<div class="container">
  <div class="row">
    <div class="col-2 text-center ">
      <h2> ID</h2>
      <hr>
      <div class="row">
        <div class="col-12 text-center ">
          <br>
          <ol class="" style="list-style-type:none">
            {{#each data}}
            <li>
              <h2>{{this.id}}</h2>
              <br>
            </li>
            {{/each}}
          </ol>
        </div>
      </div>
    </div>

    <div class="col-5 text-center ">
      <h2>Children Name</h2>
      <hr>
      <div class="row">
        <div class="col-12 text-justify ">
          <br>
          <ol class="" style="list-style-type:none">
            {{#each data}}
            <li>
              <h2>{{child_name}}</h2>
              <br>
            </li>
            {{/each}}
          </ol>
        </div>
      </div>
    </div>

    <div class="col-5 text-center ">
      <h2>Parent Name</h2>
      <hr>
      <div class="row">
        <div class="col-12 text-center ">
          <br>
          <ol class="" style="list-style-type:none">
            {{#each data}}
            <li>
              <h2>{{parent_name}}</h2>
              <br>
            </li>
            {{/each}}
          </ol>
        </div>
      </div>
    </div>
